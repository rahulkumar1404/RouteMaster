izant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\components\\UpdateBus.js",[],"C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\components\\Passengers.js",["111","112"],"/* eslint-disable jsx-a11y/anchor-is-valid */\nimport React, { Component } from 'react';\nimport BookingService from '../services/BookingService';\nimport busBG from \"../assets/images/bus.jpg\";\nimport Footer from './Footer';\nimport Header from './Header';\n\n/** \n * \n * This component will take input for passengers details\n * BookingService: Service for adding passenger details\n*/\nclass Passengers extends Component {\n\n    constructor(props){\n        super(props)\n        if(!localStorage.getItem('user')){this.props.history.push('/login')}\n        else{\n        this.service = new BookingService();\n        this.values= [] \n        this.state ={ \n            npsgn:parseInt(localStorage.getItem(\"nop\")),\n            pname:'',\n            gen:['Select','Male','Female'],\n            gender:'',\n            age:'',\n            id:1,\n            btn:false,\n            info:false\n        }\n    }\n    }\n\n    componentDidMount(){\n        // console.log(this.state.npsgn)\n    }\n\n    /** this handle event method add a single passenger at a time in array*/\n    handleClick = (idx) => {\n        this.values.push({\"id\":this.state.id++ ,\"pname\":this.state.pname, \"gender\":this.state.gender, \"age\":this.state.age})\n        console.log(\"Values :\"+ JSON.stringify(this.values.length))\n        this.setState({info:true})\n        if(this.values.length===this.state.npsgn){\n            this.setState({btn:true})\n        }\n    }\n\n    /** \n     * this method interacts with service to add passenger list \n     * stores passenger list in local storage\n     * then redirects to Summary component\n     */\n    savePassenger = () => {\n        console.log(\"Values save :\"+ JSON.stringify(this.values))\n        localStorage.setItem('sid',JSON.stringify(this.values))\n        this.service.addPassengers({\"pass1\":this.values})\n        this.props.history.push('/summary')\n    }\n\n\n\n    render() {\n    if(!localStorage.getItem('user')){return null}\n\n    var fieldsArray = [];\n\n    // looping to add passengers to fieldsArray array\n    for (var i = 0; i < this.state.npsgn; i++) {\n        console.log(\"inside for\")\n      fieldsArray.push(\n          <tr>\n            <td>\n                <input \n                class=\"form-control\"\n                type='text'\n                name='pname'\n                onChange={e => this.setState({pname:e.target.value, info:false})} />\n            </td>\n            <td> \n            <select class=\"btn btn-info\" name='gender' onChange={e => this.setState({gender:e.target.value})} >\n                {this.state.gen.map(g => (\n                    <option value={g}>{g}</option>\n                ))}\n            </select>\n                \n            </td>\n            <td>\n                <input \n                class=\"form-control\"\n                type='text' \n                name='age'\n                onChange={e => this.setState({age:e.target.value})} maxLength=\"2\" />\n            </td>\n            <td align='center'>\n                <button disabled={this.state.btn} class=\"btn btn-warning \" onClick={() => this.handleClick(i)}>Add Passenger</button>\n            </td>\n        </tr>\n      );\n    }\n\n\n    return (\n        <div class='pt-5'>\n        <Header />\n        <div class=\"py-5\" style={{backgroundImage: `url(${busBG})`, backgroundSize:'cover',overflow: 'hidden', height: '900px'}}>\n            <div class=\"row mb-4\">\n                <div class=\"col-lg-8 mx-auto text-center\">\n                    <h1 class=\"display-6\" style={{color:'black', fontWeight:'700' ,backgroundColor:'#72bedc'}}>Add Passenger Details</h1>\n                </div>\n            </div>\n\n            <div class=\"row\" >\n\t\t\t\t<div class=\"col-md-10  mx-auto\">\n\t\t\t\t\t<div class=\"card\" >\n                    <div class=\"card-header\">\n                    <div class=\"alert alert-primary\">\n                        <strong>Note: </strong> Please add passengers individually\n                    </div>\n                        <div class=\"bg-white shadow-sm pt-4 pl-2 pr-2 pb-2\">\n                            <div class=\"tab-content\">\n                            {this.state.info && <div class=\"alert alert-success\">\n                                <strong>Success!</strong>&nbsp; Passenger added with name : &nbsp; {this.state.pname}\n                            </div>}\n                                <div class=\"tab-pane fade show active pt-3\">\n                                <form>\n                                    \n                                    <table class=\"table\">\n                                        <tr align=\"center\" >\n                                            <th>Name</th>\n                                            <th>Gender</th>\n                                            <th>Age</th>\n                                            <th>Add Passenger</th>\n                                        </tr>\n                                        <tbody>\n                                            {fieldsArray}\n                                        </tbody>\n\n                                    </table>\n                                    <div class=\"checkbox\">\n                                        <label><input type=\"checkbox\" value=\"\" required />&nbsp; <a href=\"#\" data-toggle=\"modal\" data-target=\"#myModal\">Agree Terms and Conditions</a></label>\n                                    </div>\n                                    <div class=\"card-footer\"> \n                                        <button onClick={this.savePassenger} type=\"button\" disabled={!this.state.btn} class=\"subscribe btn btn-primary btn-block shadow-sm\"> Book Ticket</button>\n                                    </div>\n\n                                </form>\n                                                                        \n                                        <div class=\"modal fade\" id=\"myModal\">\n                                            <div class=\"modal-dialog modal-dialog-centered\">\n                                            <div class=\"modal-content\">\n                                            \n                                                \n                                                <div class=\"modal-header\">\n                                                <h4 class=\"modal-title\">Post COVID-19 Conditions of Carriage</h4>\n                                                <button type=\"button\" class=\"close\" data-dismiss=\"modal\">&times;</button>\n                                                </div>\n                                                \n                                            \n                                                <div class=\"modal-body\">\n                                                1. As per Government of India directive “vulnerable persons such as very elderly, pregnant ladies, passengers with health issues are advised to avoid land travel”\n<br></br>\n                                                2. Passengers to familiarize and follow the social distancing norms as required at the bus Stop premises.\n<br></br>\n                                                3. Entry into the bus Stop terminal will be permitted only with suitable PPE, at least with a face mask.\n<br></br>\n                                                4. Follow all self sanitisation norms, as applicable, at the bus Stop.\n<br></br>\n                                                5. In case any symptoms of COVID-19, passengers may be debarred from entry into the bus Stop or land travel by appropriate authorities.\n\n                                                </div>\n                                                \n                                                \n                                                <div class=\"modal-footer\">\n                                                <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close</button>\n                                                </div>\n                                                \n                                            </div>\n                                            </div>\n                                        </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <Footer />\n        </div>\n    );\n}\n}\n\nexport default Passengers;","C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\components\\BusListAdmin.js",[],"C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\components\\Login.js",[],"C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\components\\Corousal.js",["113"],"import React from 'react';\n\nimport busbannernewonee from '../assets/images/busbannernewonee.png';\n/**\n * This component renders Carousel \n */\nfunction Corousal(props) {\n    return (\n            <div className=\"carousel-inner col-lg-17\">\n                <div className=\"carousel-item active\">\n                    <img src={busbannernewonee} width=\"100%\" height=\"500\" />\n                </div>\n            </div>\n    );\n}\n\nexport default Corousal;","C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\components\\Header.js",["114","115"],"import React from 'react';\nimport { Link, useHistory } from 'react-router-dom';\n\n\n/**\n * \n * \n * This component renders header and uses condition rendering for differnt users/ non user\n */\nfunction Header(props) {\n    const history = useHistory();\n    const bususer = localStorage.getItem('user');\n\n    /** The component will be rendered when user has not signed in */\n    const loggedIn = (\n        \n            <ul className=\"nav justify-content-end\">\n             <li className=\"nav-item\">\n                <Link className=\"nav-link text-info\" to=\"/\">\n                    <button className=\"btn btn-outline-light\">Home</button>\n                </Link>\n            </li>\n                <li className=\"nav-item\">\n                <Link className=\"nav-link text-info\" to=\"/login\">\n                    <button className=\"btn btn-outline-light\">Login</button>\n                </Link>\n                </li>\n                <li className=\"nav-item\">\n                <Link className=\"nav-link text-info\" to=\"/register\">\n                    <button className=\"btn btn-outline-light\">Register</button>\n                </Link>\n                </li>\n            </ul>\n        \n    );\n\n    // clearing local storage for given items when user logs out\n    const userClear = () => (\n        localStorage.removeItem('user'),\n        localStorage.removeItem('plane'),\n        localStorage.removeItem('bid'),\n        localStorage.removeItem('sid'),\n        localStorage.removeItem('tickets'),\n        localStorage.removeItem('nop'),\n        localStorage.removeItem('ticket'),\n        localStorage.clear()\n    )\n\n    /** Redirecting to booking history */\n    const onTickets= () => {\n       history.push('/tickets')\n    }\n\n    /** The component will be rendered when user is signed in */\n    const loggedOut = (\n            <ul className=\"nav justify-content-end\">\n                {/* {console.log(JSON.parse(localStorage.getItem('user')).isadmin)} */}\n               \n\n                <li className=\"nav-item\">\n                    <Link className=\"nav-link text-info\" to=\"/\">\n                        <button className=\"btn btn-outline-light\">Home</button>\n                    </Link>\n                </li>\n                { localStorage.getItem('user') && JSON.parse(localStorage.getItem('user')).isadmin ===0 \n                &&  \n                    <li className=\"nav-item nav-link text-info\">\n                            <button onClick={onTickets} className=\"btn btn-outline-light\">BookingHistory</button>\n                    </li>\n                }\n\n                {localStorage.getItem('user') && JSON.parse(localStorage.getItem('user')).isadmin ===1 \n                &&\n                <li className=\"nav-item\">\n                    <Link className=\"nav-link text-info\" to=\"/addBus\">\n                        <button className=\"btn btn-outline-light\">Add Bus</button>\n                    </Link>\n                </li>}\n\n\n                {localStorage.getItem('user') && JSON.parse(localStorage.getItem('user')).isadmin ===1\n                &&\n                <li className=\"nav-item\">\n                    <Link className=\"nav-link text-info\" to=\"/allBuses\">\n                        <button className=\"btn btn-outline-light\">All Buses</button>\n                    </Link>\n                </li>\n                }\n\n                <li className=\"nav-item\">\n                    <Link className=\"nav-link text-info\" to=\"/\">\n                        <button onClick={userClear} className=\"btn btn-outline-light\">Logout</button>\n                    </Link>\n                </li>\n\n                { localStorage.getItem('user') && JSON.parse(localStorage.getItem('user')).isadmin ===0 \n                &&  \n                <li className=\"nav-item nav-link text-info\">\n                    <h6 style={{marginTop:'7px'}}><b className='text-warning'>Welcome, {JSON.parse(localStorage.getItem('user')).username}</b></h6>\n                </li>\n                }\n\n                \n                {localStorage.getItem('user') && JSON.parse(localStorage.getItem('user')).isadmin ===1\n                &&\n                <li className=\"nav-item\">\n                    <Link className=\"nav-link text-info\" to=\"/admin\">\n                        <button className=\"btn btn-outline-info\">Admin</button>\n                    </Link>\n                </li>\n                }\n\n\n            </ul>\n    );\n    \n\n    return (\n        <div>\n            <nav className=\"navbar navbar-dark bg-nav fixed-top\" style={navstyle.bg}>\n                <div className=\"container-fluid\">\n                    <Link style={navstyle.navbar_brand} to=\"/\">\n                        RouteMaster\n                    </Link>\n                       \n\n                    { localStorage.getItem('user')  ? loggedOut : loggedIn  }\n                    {/* {console.log(localStorage.getItem('user'))} */}\n                   \n                </div>\n            </nav>\n        </div>\n    );\n};\n\nlet navstyle = {\n    bg : {\n        backgroundColor : \"#000033\",\n    },\n    navbar_brand : {\n        color: \"#ffffff\",\n        fontSize: \"x-large\",\n        fontFamily: \"Verdana\",\n        fontStyle: \"bold\",\n        textDecoration: \"none\"\n    }\n\n}\n\nexport default Header;","C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\components\\Footer.js",[],"C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\components\\SearchBus.js",["116"],"import React, { Component } from 'react';\nimport BusList from './BusList';\nimport BusServiceRest from '../services/BusServiceRest';\nimport search from '../assets/logo/magnifying-glass.png';\n\n/**\n * \n * This component searches bus based on source, destibnation and date\n * BusServiceRest: Service for fetching buses\n */\nclass SearchBus extends Component {\n    constructor(props) {\n        super(props);\n        this.service = new BusServiceRest();\n        this.flag = false\n        this.state = {\n            source : \"\",\n            destination: \"\",\n            travelDate: undefined,\n            searched : false\n        }\n    }\n\n    componentDidMount(){\n        this.setState({\n            searched: false\n        })\n    }\n\n    handleInput = (event) => {\n        const name = event.target.name;\n        const value = event.target.value;\n        this.setState({\n            [name]:value\n        })\n    }\n\n    /**\n     * This method interact with service to fetch buses details\n     */\n    getBusesList = () => {\n        this.setState({\n            searched: false\n        });\n        const s = this.state.source;\n        const d = this.state.destination;\n        const t = this.state.travelDate;\n\n        this.service.getBusesForUser(s,d,t).then(data => {\n            if(data.length>0){\n                this.setState({\n                    buses : data,\n                    searched : true\n                });\n               \n            }\n            else{\n                alert('No Buses Found!!')\n        }\n            // console.log(this.state.buses);\n        })\n    }\n\n    render() {\n        return (\n            <div className=\"container-fluid p-4 m-3\">\n                <h2 style={styling.heading}>Search for buses</h2>\n                <div className=\"form-inline\">\n                    <div className=\"input-group mb-2 mr-sm-2\">\n                        {/* <!-- Drop down for source --> */}\n                        <select className=\"custom-select my-1 mr-sm-2\" id=\"source\" name=\"source\" onClick={this.handleInput} required>\n                            <option value=\"Chennai\" onClick={this.handleInput}>Chennai</option>\n                            <option value=\"Delhi\" onClick={this.handleInput}>Delhi</option>\n                            <option value=\"Mumbai\" onClick={this.handleInput}>Mumbai</option>\n                            <option value=\"Kolkata\" onClick={this.handleInput}>Kolkata</option>\n                            <option value=\"Goa\" onClick={this.handleInput}>Goa</option>\n                            <option value=\"Pune\" onClick={this.handleInput}>Pune</option>\n                            <option value=\"Jaipur\" onClick={this.handleInput}>Jaipur</option>\n                            <option value=\"Bangalore\" onClick={this.handleInput}>Bangalore</option>\n                            <option value=\"Cochin\" onClick={this.handleInput}>Cochin</option>\n                            <option value=\"Ahmadabad\" onClick={this.handleInput}>Ahmadabad</option>\n                        </select>\n                    </div>    \n                        {/* <!-- Drop down for destination --> */}\n                    <div className=\"input-group mb-2 mr-sm-2\">\n                        <select className=\"custom-select my-1 mr-sm-2\" name=\"destination\" onClick={this.handleInput} required>\n                            <option value=\"Chennai\" onClick={this.handleInput}>Chennai</option>\n                            <option value=\"Delhi\" onClick={this.handleInput}>Delhi</option>\n                            <option value=\"Mumbai\" onClick={this.handleInput}>Mumbai</option>\n                            <option value=\"Kolkata\" onClick={this.handleInput}>Kolkata</option>\n                            <option value=\"Goa\" onClick={this.handleInput}>Goa</option>\n                            <option value=\"Pune\" onClick={this.handleInput}>Pune</option>\n                            <option value=\"Jaipur\" onClick={this.handleInput}>Jaipur</option>\n                            <option value=\"Bangalore\" onClick={this.handleInput}>Bangalore</option>\n                            <option value=\"Cochin\" onClick={this.handleInput}>Cochin</option>\n                            <option value=\"Ahmadabad\" onClick={this.handleInput}>Ahmadabad</option>\n                        </select>\n                    </div>    \n                        \n                    <div className=\"input-group mb-2 mr-sm-2\">    \n                        <input className=\"my-1 p-2 border border-darken-2 rounded\" type=\"date\" value={this.state.travelDate}\n                        name=\"travelDate\" onChange={this.handleInput} required />\n                    </div>\n                    <button onClick={this.getBusesList} className=\"btn mb-2 mr-sm-2\" style={styling.icon}>\n                        <img src={search} height=\"25\" />\n                    </button>\n                </div>\n \n                {this.state.searched && <BusList buses={this.state.buses}  />}\n               \n            </div>\n        );\n    }\n}\n\nlet styling = {\n    heading : {\n        color : \"#333333\",\n        fontFamily : \"Verdana\",\n        marginBottom : 20\n    },\n    icon : {\n       \n    }\n}\n\nexport default SearchBus;","C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\services\\BookingService.js",[],"C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\services\\UserService.js",[],"C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\services\\BusServiceRest.js",[],"C:\\Users\\2317761\\OneDrive - Cognizant\\RouteMaster-reactjs-springboot\\RouteMaster_UI\\src\\components\\BusList.js",[],{"ruleId":"117","severity":1,"message":"118","line":6,"column":25,"nodeType":"119","messageId":"120","endLine":6,"endColumn":30},{"ruleId":"117","severity":1,"message":"121","line":6,"column":32,"nodeType":"119","messageId":"120","endLine":6,"endColumn":38},{"ruleId":"122","severity":1,"message":"123","line":17,"column":39,"nodeType":"124","messageId":"125","endLine":17,"endColumn":40},{"ruleId":"126","severity":1,"message":"127","line":40,"column":32,"nodeType":"128","messageId":"129","endLine":40,"endColumn":42},{"ruleId":"130","severity":1,"message":"131","l